<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.muhmmadnabil.a7minutesworkout">

    <queries>
        <intent>
            <action android:name="android.intent.action.TTS_SERVICE" />
        </intent>
    </queries>

    <application
        android:name=".WorkOutApp"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.7MinutesWorkout">
        <activity
            android:name=".activies.history.HistoryActivity"
            android:exported="false" />
        <activity
            android:name=".activies.bmi.BmiActivity"
            android:exported="false" />
        <activity
            android:name=".activies.finish.FinishActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".activies.exercise.ExerciseActivity"
            android:exported="false" />
        <activity
            android:name=".activies.main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>